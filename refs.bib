@article{doi:10.2352/ISSN.2470-1173.2018.01.VDA-333,
	abstract = {
        Phase space tessellation techniques for N&#x2013;body dark matter simulations yield density fields of very high quality. However, due to the vast amount of elements and self-intersections in the resulting tetrahedral meshes, interactive visualization methods for this approach so far
 either employed simplified versions of the volume rendering integral or suffered from rendering artifacts. This paper presents a volume rendering approach for phase space tessellations, that combines state-of-the-art order&#x2013;independent transparency methods to manage the extreme depth
 complexity of this mesh type. We propose several performance optimizations, including a view&#x2013;dependent multiresolution representation of the data and a tile&#x2013;based rendering strategy, to enable high image quality at interactive frame rates for this complex data structure and demonstrate
 the advantages of our approach for different types of dark matter simulations.
      },
	author = {Ralf Kaehler},
	doi = {10.2352/ISSN.2470-1173.2018.01.VDA-333},
	journal = {Electronic Imaging},
	keywords = {Volume Rendering, Astrophysics, Dark Matter Simulations},
	number = {1},
	pages = {333-1--333-1},
	title = {High-Quality Volume Rendering of Dark Matter Simulations},
	url = {https://library.imaging.org/ei/articles/30/1/art00005},
	volume = {30},
	year = {2018},
	bdsk-url-1 = {https://library.imaging.org/ei/articles/30/1/art00005},
