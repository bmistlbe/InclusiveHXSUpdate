	eprint = {1408.7084},
	month = {aug},
	title = {{Measurement of Higgs boson production in the diphoton decay channel in $pp$ collisions at center-of-mass energies of 7 and 8 TeV with the ATLAS detector}},
	url = {http://arxiv.org/abs/1408.7084 http://dx.doi.org/10.1103/PhysRevD.90.112015},
	year = {2014},
	bdsk-url-1 = {http://arxiv.org/abs/1408.7084%20http://dx.doi.org/10.1103/PhysRevD.90.112015},
